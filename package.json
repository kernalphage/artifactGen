{
  "name": "antique_gen",
  "version": "1.0.0",
  "description": "procedural text and json generator based on twine and dwarf fortress",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "browserify index.js --debug -o dist/bundle.js",
    "watch": "watchify index.js --debug -o dist/bundle.js -v"
  },
  "author": "matt dobler",
  "license": "ISC",
  "jest":{
    "moduleFileExtensions": ["js", "jsx"],
    "moduleDirectories": ["node_modules", "bower_components", "shared"],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "babel":{
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.3.0",
    "watchify": "^3.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "jest": "^24.8.0"
  }
}
